{
  "expo": {
    "scheme": "thoughtmarks",
    "entryPoint": "App.tsx",
    "name": "mobile-native-fresh",
    "slug": "mobile-native-fresh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#181818"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#181818"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-font"
    ],
    "extra": {
      "googleIosClientId": "1234567890-abcdefgHIJKLMNopqrstuv.apps.googleusercontent.com",
      "googleAndroidClientId": "1234567890-ABCDEFGhijklmnopqrst.apps.googleusercontent.com",
      "googleWebClientId": "1234567890-abcdefghIjklmnopQRs.apps.googleusercontent.com"
    }
  }
}
